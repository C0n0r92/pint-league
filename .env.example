# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Firebase (for FCM push notifications)
# Note: google-services.json and GoogleService-Info.plist handle most Firebase config
# This is only needed for server-side FCM v1 API
FIREBASE_PROJECT_ID=pints-league

# TrueLayer (Open Banking)
TRUELAYER_CLIENT_ID=your_truelayer_client_id
TRUELAYER_CLIENT_SECRET=your_truelayer_client_secret

# Google Places API (for pub enrichment)
GOOGLE_PLACES_API_KEY=your_google_places_api_key

# Encryption key for bank tokens (generate a random 32-character string)
TOKEN_ENCRYPTION_KEY=your_32_character_encryption_key

